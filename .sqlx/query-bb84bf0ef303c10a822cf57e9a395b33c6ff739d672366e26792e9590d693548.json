  "query": "\n                    SELECT book.id, book.title, book.author, book.price, book.isbn, cart_items.quantity\n                    FROM books book\n                    JOIN cart_items ON book.id = cart_items.bookId\n                    JOIN user_cart ON cart_items.cartId = user_cart.id\n                    WHERE user_cart.userId = ?\n                    ",
